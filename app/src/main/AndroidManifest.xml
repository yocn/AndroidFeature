<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yocn.af">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.ASMActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.ColorFadeInActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.ColorFadeInActivity2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.TestWindowActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.taskStack.TestStackActivity"
            android:launchMode="standard"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.taskStack.StandardActivity"
            android:launchMode="standard"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.taskStack.SingleTopActivity"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.taskStack.SingleTaskActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.yocn.af"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.taskStack.SingleInstanceActivity"
            android:launchMode="singleInstance"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.TouchEventDispatchActivity"
            android:launchMode="standard"
            android:theme="@style/AppTheme" />
    </application>

</manifest>